body{font-family:comic neue,sans-serif;margin:auto;background:#1f2f0a;color:#9bc53d}body ::selection{background:#9bc53d;color:#1f2f0a}body a{color:#9bc53d;text-decoration:underline}body a:hover{font-weight:700}main-content{margin:0;padding:0 10px;grid-column:1;grid-row:2;background:rgba(31,47,10,.5);text-align:justify;text-justify:inter-word}main-content img{display:block;margin-left:auto;margin-right:auto;width:60%}nav ul{list-style:none;margin:0;padding:9px 5px;font-size:1.5em;text-align:center;background:#9bc53d}nav ul ::selection{background:#1f2f0a;color:#a07700}nav ul a{color:#1f2f0a;text-decoration:none}nav ul a:hover{font-weight:700}nav ul li{display:inline;background:#a07700}nav{grid-column:1;grid-row:1}footer{margin:0 0 auto;padding:0 10px;font-size:.8em;background:#9bc53d;text-align:center;color:#1f2f0a;grid-column:1;grid-row:3}footer ::selection{background:#1f2f0a;color:#9bc53d}header{grid-column:1/4;grid-row:1;font-size:2.5em;text-align:center;font-weight:700}.container{display:grid;grid-template-columns:1fr 2fr 1fr;grid-auto-rows:minmax(50px,auto);gap:10px;width:1000px;margin:auto}log{margin:0 0 auto;padding:0 10px;grid-column:1;grid-row:1;background:rgba(31,47,10,.5);padding:0 10px 10px;text-align:left}log h2{text-align:center;font-size:1em}.log-scroll{height:200px;overflow:scroll;scrollbar-color:#9bc53d #1F2F0A}music{margin:0 0 auto;padding:0 10px;grid-column:1;grid-row:2;background:rgba(31,47,10,.5);text-align:justify}music audio{display:block;margin-left:auto;margin-right:auto;height:50px;width:100%;background:#9bc53d}music img{display:block;margin-left:auto;margin-right:auto;width:100%}butts{margin:0;padding:0 10px;grid-column:1;grid-row:1;background:rgba(31,47,10,.5);text-align:center}blinkies{margin:0 0 auto;padding:0 10px;grid-column:1;grid-row:2;background:rgba(31,47,10,.5);text-align:center}.sidebar-left{margin:0 0 auto;grid-column:1;grid-row:2/5;display:grid;grid-auto-rows:minmax(100px,auto);gap:inherit}.sidebar-right{margin:0 0 auto;grid-column:3;grid-row:2/5;display:grid;grid-auto-rows:minmax(100px,auto);gap:inherit}.middle{grid-column:2;grid-row:2;display:grid;grid-auto-rows:minmax(50px,auto);gap:inherit}